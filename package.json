{
  "name": "my-zhidian-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --hostname 0.0.0.0 --port 3007",
    "lint": "next lint",
    "start": "next start",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:e2e": "cross-env PLAYWRIGHT_TEST=true playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen http://localhost:3007",
    "test:e2e:report": "playwright show-report",
    "test:e2e:concurrent": "cross-env PLAYWRIGHT_TEST=true CONCURRENT_WORKERS=8 playwright test e2e/concurrent-stress.spec.ts",
    "test:e2e:stress": "cross-env PLAYWRIGHT_TEST=true CONCURRENT_WORKERS=12 playwright test e2e/concurrent-stress.spec.ts --workers=12",
    "test:e2e:performance": "cross-env PLAYWRIGHT_TEST=true CONCURRENT_WORKERS=6 playwright test e2e/performance-only.spec.ts --workers=6",
    "test:e2e:performance:fast": "cross-env PLAYWRIGHT_TEST=true CONCURRENT_WORKERS=10 playwright test e2e/performance-only.spec.ts --workers=10",
    "test:e2e:extreme": "cross-env CONCURRENT_WORKERS=300 playwright test e2e/extreme-stress.spec.ts --config=playwright-extreme.config.ts --workers=50",
    "test:e2e:extreme:full": "cross-env CONCURRENT_WORKERS=300 playwright test e2e/extreme-stress.spec.ts --config=playwright-extreme.config.ts --workers=100",
    "test:e2e:extreme:safe": "cross-env CONCURRENT_WORKERS=150 playwright test e2e/extreme-stress.spec.ts --config=playwright-extreme.config.ts --workers=30",
    "test:e2e:extreme:debug": "cross-env CONCURRENT_WORKERS=50 playwright test e2e/extreme-stress.spec.ts --config=playwright-extreme.config.ts --workers=10 --debug",
    "test:e2e:basic": "cross-env PLAYWRIGHT_TEST=true CONCURRENT_WORKERS=1 playwright test e2e/chat.spec.ts --workers=1",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "import:merchants": "npx tsx scripts/import-merchant-data.ts",
    "verify:merchants": "npx tsx scripts/verify-import.ts",
    "test:features": "npx tsx scripts/test-merchant-features.ts",
    "dev:fast": "next dev --hostname 0.0.0.0 --port 3007 --turbo",
    "dev:debug": "NODE_OPTIONS=\"--inspect\" next dev --hostname 0.0.0.0 --port 3007",
    "rollback": "node scripts/rollback.js disable",
    "rollback:full": "node scripts/rollback.js full",
    "monitor": "node scripts/performance-monitor.js",
    "env:validate": "node scripts/env-config.js validate",
    "env:toggle": "node scripts/env-config.js toggle",
    "test:phase0": "node scripts/automated-test-runner.js phase 0",
    "test:phase1": "node scripts/automated-test-runner.js phase 1",
    "test:phase2": "node scripts/automated-test-runner.js phase 2",
    "test:phase3": "node scripts/automated-test-runner.js phase 3",
    "test:full": "node scripts/automated-test-runner.js full",
    "security:check": "node scripts/security-check.js",
    "security:check:prod": "node scripts/security-check.js --production",
    "security:clean": "node scripts/remove-dev-configs.js --force",
    "security:clean:dry": "node scripts/remove-dev-configs.js --dry-run",
    "pre-deploy": "node scripts/pre-deploy.js",
    "deploy:validate": "npm run security:check:prod && npm run pre-deploy",
    "build:safe": "npm run deploy:validate && npm run build",
    "build:prod": "NODE_ENV=production npm run build:safe",
    "health:check": "node scripts/project-health-check.js",
    "health:quick": "node scripts/project-health-check.js --quick",
    "debt:track": "node scripts/project-health-check.js --debt-only",
    "backup:db": "node scripts/backup-database.js",
    "backup:full": "node scripts/backup-database.js --type=full --compress",
    "backup:schema": "node scripts/backup-database.js --type=schema",
    "backup:auto": "node scripts/auto-backup-scheduler.js",
    "backup:test": "node scripts/auto-backup-scheduler.js --test",
    "restore:db": "node scripts/restore-database.js",
    "restore:help": "node scripts/restore-database.js --help",
    "mcp:check": "node scripts/mcp-manager.js check",
    "mcp:config": "node scripts/mcp-manager.js config",
    "mcp:install": "node scripts/mcp-manager.js install",
    "mcp:start-server": "node scripts/mcp-manager.js start-server",
    "mcp:test-api": "node scripts/mcp-manager.js test-api"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
